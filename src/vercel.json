{
  "version": 2,
  "builds": [
    { "src": "dist/my-portfolio/server/server.mjs", "use": "@vercel/node" },
    { "src": "dist/my-portfolio/browser/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "dist/my-portfolio/server/server.mjs" }
  ]
}
